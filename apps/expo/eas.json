{
  "cli": {
    "version": ">= 3.8.1"
  },
  "build": {
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env":{
        "EXPO_PUBLIC_MEDUSA_BACKEND_URL":"https://alpha.sourcegoodfood.com",
        "EXPO_USE_METRO_WORKSPACE_ROOT":"1"
      },
      
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "preview3": {
      "developmentClient": true
    },
    "production": {
      "env":{
        "EXPO_PUBLIC_MEDUSA_BACKEND_URL":"https://alpha.sourcegoodfood.com",
        "EXPO_USE_METRO_WORKSPACE_ROOT":"1"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {

      }
    }
    
  },
  "submit": {
    "production": {}
  }
}
